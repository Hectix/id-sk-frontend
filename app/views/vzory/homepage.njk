{% block styles %}
<style>
  .idsk-intro-block__side-menu__ul {
    list-style: none;
  }
  .app-pane-blue .idsk-intro-block__side-menu__li {
    padding: 10px 0;
  }
  .idsk-intro-block__side-menu__li:before {
    content: "\2022";
    color: white;
    display: inline-block;
    margin-left: 0;
    font-size: 40px;
    margin-right: 10px;
    line-height: 20px;
  }

  .idsk-events-list {
    font-family: "Source Sans Pro", "Arial", sans-serif;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    flex-wrap: wrap;
  }
  .idsk-events-list > .govuk-grid-column-one-third {
    display: flex;
    flex-direction: column;
    padding-left: 0;
  }
  .idsk-events-list__block{
    height: 100%;
    position: relative;
  }
  .idsk-events-list__event-title{
    margin-bottom: 40px;
  }
  .idsk-events-list__event-title .govuk-link {
    text-decoration: none;
  }
  .idsk-events-list__event-desc{
    position: absolute;
    height: auto;
    width: 100%;
    bottom: 15px;
   }

  .idsk-blue-banner{
    background-color: #003078;
    padding: 30px 0;
    margin: 30px 0;
    height: 100%;
    position: relative;
    display: inline-block;
  }

  .idsk-blue-banner .govuk-heading-l,
  .idsk-blue-banner .govuk-caption-l,
  .idsk-blue-banner .govuk-link {
    color: white;
  }
</style>
{% endblock %}

